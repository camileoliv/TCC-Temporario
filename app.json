{
  "expo": {
    "name": "Temoco",
    "slug": "temoco",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "temoco",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "androidNavigationBar": {
        "visible": "immersive",
        "backgroundColor": "#000000"
      },
      "jsEngine": "jsc",
      "edgeToEdgeEnabled": true,
      "package": "com.temoco.app",
      "permissions": [
        "INTERNET",
        "VIBRATE",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/Tela_inicial_Logo.png",
          "imageWidth": 700,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8bb30a5f-7c16-4f30-b641-2a0a4e073751"
      }
    },
    "owner": "camileolivv"
  }
}
