{
  "expo": {
    "name": "Temoco",
    "slug": "temoco",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "temoco",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/Tela_inicial_Logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "jsEngine": "hermes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "androidNavigationBar": {
        "visible": "immersive",
        "backgroundColor": "#000000"
      },
      "jsEngine": "hermes",
      "edgeToEdgeEnabled": true,
      "package": "com.temoco.app",
      "permissions": ["INTERNET", "VIBRATE"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-screen-orientation", { "initialOrientation": "PORTRAIT" }],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "47e520d1-35a8-4e00-9435-65e78b8e3b72"
      }
    },
    "owner": "camileolivv"
  }
}